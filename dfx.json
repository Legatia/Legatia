      "wasm": "backend/canisters/relationships/target/wasm32-unknown-unknown/release/relationships.wasm"
    },
    "invites": {
      "type": "custom",
      "candid": "backend/candid/invites.did",
      "build": "cargo build --target wasm32-unknown-unknown --release -p invites",
